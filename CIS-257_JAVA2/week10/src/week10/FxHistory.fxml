<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="week10.FxHistoryController">
   <children>
      <VBox prefHeight="245.0" prefWidth="320.0">
         <children>
            <HBox />
            <TextArea fx:id="txtHistory" prefHeight="200.0" prefWidth="300.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </padding>
            </TextArea>
         </children>
      </VBox>
      <Button fx:id="btnClear" layoutX="70.0" layoutY="214.0" mnemonicParsing="false" text="Clear" />
      <Button fx:id="btnHide" layoutX="22.0" layoutY="214.0" mnemonicParsing="false" text="Hide" />
   </children>
</AnchorPane>
